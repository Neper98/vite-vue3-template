	"dependencies": {
		"axios": "0.22.0",
		"core-js": "3.18.2",
		"cropperjs": "1.5.12",
		"crypto-js": "4.1.1",
		"element-plus": "1.1.0-beta.19",
		"nprogress": "0.2.0",
